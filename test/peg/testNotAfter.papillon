{-# LANGUAGE QuasiQuotes #-}

import Text.Papillon

main :: IO ()
main = case dv_something $ parse "abc\ndefn" of
	Right (r, _) -> print r
	Left err -> putStrLn $ "not match: " ++ err

true :: Bool
true = True

cons :: a -> [a] -> [a]
cons = (:)
emp :: [a]
emp = []

[papillon|

something :: String
	= !'\n' c s:something	{ cons c s }
	/				{ emp }
;

|]
