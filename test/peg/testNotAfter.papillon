{-# LANGUAGE QuasiQuotes #-}

import Text.Papillon

main :: IO ()
main = case dv_some $ parse "abc\ndefn" of
	Right (r, _) -> print r
	Left err -> putStrLn $ "not match: " ++ err

true :: Bool
true = True

cons :: a -> [a] -> [a]
cons = (:)
empty :: [a]
empty = []

[papillon|

some :: String
	= !'\n' c s:some	{ cons c s }
	/				{ empty }
;

|]
