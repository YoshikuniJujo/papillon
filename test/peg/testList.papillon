{-# LANGUAGE QuasiQuotes, UndecidableInstances, TypeFamilies #-}

import Text.Papillon

main :: IO ()
main = case dv_other $ parse initialPos "something" of
	Right (r, _) -> print r
	Left err -> putStrLn $ "not match " ++ show err

[papillon|

something :: Char
	= c		{ c }
;
other :: String
	= s:(c:something { c })* { s }
;

|]
